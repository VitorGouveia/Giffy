  "license": "MIT",
  "private": true,
  "main": "./build/index.js",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "dev": "rimraf dist && tsc -w",
    "tsc": "rimraf dist && tsc",
    "build": "rimraf build && babel src --extensions \".js,.ts,.tsx\" --out-dir build --copy-files --no-copy-ignored && tsc",
    "bundle": "webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.10",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-centered": "^5.3.21",
    "@storybook/addon-docs": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-viewport": "^6.4.14",
    "@storybook/addons": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@storybook/theming": "^6.4.14",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "babel-preset-minify": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.2.1",
    "react-icons": "^4.3.1",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@giffy/styles": "*",
    "release-it": "^14.12.3",
    "rimraf": "^3.0.2"
  }
